    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.0",
    "autoprefixer": "^10.4.16",
    "electron": "^27.1.0",
    "electron-builder": "^24.6.4",
    "electron-vite": "^1.0.29",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
